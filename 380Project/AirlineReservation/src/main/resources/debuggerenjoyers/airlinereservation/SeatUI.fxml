<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="602.0" prefWidth="772.0"
            xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="debuggerenjoyers.airlinereservation.SeatController">

    <children>
        <TextField fx:id="firstNameTextField" layoutX="49.0" layoutY="165.0" promptText="Passenger First Name"/>
        <TextField fx:id="lastNameTextField" layoutX="49.0" layoutY="201.0" promptText="Passenger Last Name"/>
        <TextField fx:id="dobTextField" layoutX="49.0" layoutY="235.0" promptText="Enter DOB: MM/DD/YYYY"/>
        <Button fx:id="submitButton" layoutX="367.0" layoutY="494.0" mnemonicParsing="false" prefHeight="25.0"
                prefWidth="140.0" text="Submit"/>
        <Button fx:id="nextButton" layoutX="198.0" layoutY="494.0" mnemonicParsing="false" prefHeight="25.0"
                prefWidth="140.0" text="Next"/>
        <ScrollBar orientation="VERTICAL"/>
        <Text fx:id="ticketsText" layoutX="58.0" layoutY="54.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Select Number Of Tickets" wrappingWidth="160.62109375"/>
        <Text fx:id="passengerPageText" layoutX="344.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Passenger Page"/>
        <Text fx:id="bagsText" layoutX="58.0" layoutY="113.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Select Number Of Bags" wrappingWidth="160.62109375"/>
        <ComboBox fx:id="ticketsComboBox" layoutX="48.0" layoutY="67.0" prefWidth="150.0"/>
        <ComboBox fx:id="bagsComboBox" layoutX="48.0" layoutY="127.0" prefWidth="150.0"/>
        <Spinner fx:id="ticketsSpinner" layoutX="49.0" layoutY="68.0"/>
        <Spinner fx:id="bagsSpinner" layoutX="49.0" layoutY="124.0"/>
    </children>
</AnchorPane>
